with(Clifford);
B:=linalg[diag](1,1,1,1,-1);
eval(makealiases(5,&quot;ordered&quot;));
e0:=-1/2*e4 + 1/2*e5;
einf:=e4+e5;

gaalopinitialize := proc()
	global blade;
	
	blade[1] := 1:
	
	blade[2] := e1:
	blade[3] := e2:
	blade[4] := e3:
	blade[5] := einf:
	blade[6] := e0:

	blade[7]  := e1 &amp;w e2:
	blade[8]  := e1 &amp;w e3:
	blade[9]  := e1 &amp;w einf:
	blade[10] := e1 &amp;w e0:
	blade[11] := e2 &amp;w e3:
	blade[12] := e2 &amp;w einf:
	blade[13] := e2 &amp;w e0:
	blade[14] := e3 &amp;w einf:
	blade[15] := e3 &amp;w e0:
	blade[16] := einf &amp;w e0:

	blade[17] := e1 &amp;w e2 &amp;w e3:
	blade[18] := e1 &amp;w e2 &amp;w einf:
	blade[19] := e1 &amp;w e2 &amp;w e0:
	blade[20] := e1 &amp;w e3 &amp;w einf:
	blade[21] := e1 &amp;w e3 &amp;w e0:
	blade[22] := e1 &amp;w einf &amp;w e0:
	blade[23] := e2 &amp;w e3 &amp;w einf:
	blade[24] := e2 &amp;w e3 &amp;w e0:
	blade[25] := e2 &amp;w einf &amp;w e0:
	blade[26] := e3 &amp;w einf &amp;w e0:

	blade[27] := e1 &amp;w e2 &amp;w e3 &amp;w einf:
	blade[28] := e1 &amp;w e2 &amp;w e3 &amp;w e0:
	blade[29] := e1 &amp;w e2 &amp;w einf &amp;w e0:
	blade[30] := e1 &amp;w e3 &amp;w einf &amp;w e0:
	blade[31] := e2 &amp;w e3 &amp;w einf &amp;w e0:

	blade[32] := e1 &amp;w e2 &amp;w e3 &amp;w einf &amp;w e0:
end:

simple:=proc(val)
  local temp;
  temp := val,[e4,e5];
  RETURN(temp);
end:

gaalop:=proc(label::evaln) 
  local coefficient,val,i,j,coeffpre,test;
  global coeff_list,coeff_list_size;
  val := eval(label);

  eval(simplify(coeff(val,e23)));

  coefficient[1] := simplify(scalarpart(simplify(val))); 
  
  coefficient[2] := simplify(coeff(val,e1)); 
  coefficient[3] := simplify(coeff(val,e2));
  coefficient[4] := simplify(coeff(val,e3));
  coefficient[5] := simplify((coeff(val,e4)+coeff(val,e5))/2);
  coefficient[6] := simplify(coeff(val,e5)-coeff(val,e4));

  coefficient[7] := simplify(coeff(val,e12));
  coefficient[8] := simplify(coeff(val,e13));
  coefficient[9] := simplify((coeff(val,e14)+coeff(val,e15))/2);
  coefficient[10] := simplify(coeff(val,e15)-coeff(val,e14));
  coefficient[11] := simplify(coeff(val,e23));
  
  coefficient[12] := simplify((coeff(val,e24)+coeff(val,e25))/2);
  coefficient[13] := simplify(coeff(val,e25)-coeff(val,e24));
  coefficient[14] := simplify((coeff(val,e34)+coeff(val,e35))/2);
  coefficient[15] := simplify(coeff(val,e35)-coeff(val,e34));
  coefficient[16] := simplify(coeff(val,e45));

  coefficient[17] := simplify(coeff(val,e123));
  coefficient[18] := simplify((coeff(val,e124)+coeff(val,e125))/2);
  coefficient[19] := simplify(coeff(val,e125)-coeff(val,e124));
  coefficient[20] := simplify((coeff(val,e134)+coeff(val,e135))/2);
  coefficient[21] := simplify((coeff(val,e135)-coeff(val,e134)));
  coefficient[22] := simplify(coeff(val,e145));
  coefficient[23] := simplify((coeff(val,e234)+coeff(val,e235))/2);
  coefficient[24] := simplify(coeff(val,e235)-coeff(val,e234));
  coefficient[25] := simplify(coeff(val,e245));
  coefficient[26] := simplify(coeff(val,e345));

  coefficient[27] := simplify((coeff(val,e1234)+coeff(val,e1235))/2);
  coefficient[28] := simplify(coeff(val,e1235)-coeff(val,e1234));
  coefficient[29] := simplify(coeff(val,e1245));
  coefficient[30] := simplify(coeff(val,e1345));
  coefficient[31] := simplify(coeff(val,e2345));

  coefficient[32] := simplify(coeff(val,e12345));
    
  coeff_list_size := 0;
  
<Font encoding="UTF-8">  printf(`gaaloparray(%s%s);\134n`,label,_opt);
</Font>  for i from 1 to 32 do
    if not coefficient[i]=0 then 
	  coeff_list_size := coeff_list_size+1;
<Font encoding="UTF-8">	  printf(`%s%s[%d] := %a;\134n`,label,_opt,i,coefficient[i]);
</Font>	  coeff_list[coeff_list_size] := i;
	end if;
  end do;

  if coeff_list_size &gt; 0 then 
<Font encoding="UTF-8">	cliffordperformant(label); 
  else printf(`%s%s[1] := 0;\134n`,label,_opt);
</Font>  end if;
  
  RETURN();
end:

Gaalop:=x-&gt;gaalop(x);

cliffordperformant:=proc(y::evaln)
  global coeff_list,coeff_list_size;
  local ausgabe,i,k,clucalc_blades;
  
  clucalc_blades := [&quot;1.0&quot;,&quot;e1&quot;,&quot;e2&quot;,&quot;e3&quot;,&quot;einf&quot;,&quot;e0&quot;,&quot;(e1^e2)&quot;,&quot;(e1^e3)&quot;,&quot;(e1^einf)&quot;,&quot;(e1^e0)&quot;,&quot;(e2^e3)&quot;,&quot;(e2^einf)&quot;,&quot;(e2^e0)&quot;,&quot;(e3^einf)&quot;,&quot;(e3^e0)&quot;,&quot;(einf^e0)&quot;,&quot;(e1^e2^e3)&quot;,&quot;(e1^e2^einf)&quot;,&quot;(e1^e2^e0)&quot;,&quot;(e1^e3^einf)&quot;,&quot;(e1^e3^e0)&quot;,&quot;(e1^einf^e0)&quot;,&quot;(e2^e3^einf)&quot;,&quot;(e2^e3^e0)&quot;,&quot;(e2^einf^e0)&quot;,&quot;(e3^einf^e0)&quot;,&quot;(e1^e2^e3^einf)&quot;,&quot;(e1^e2^e3^e0)&quot;,&quot;(e1^e2^einf^e0)&quot;,&quot;(e1^e3^einf^e0)&quot;,&quot;(e2^e3^einf^e0)&quot;,&quot;(e1^e2^e3^einf^e0)&quot;];

  ausgabe := sprintf(&quot;%s := &quot;,y);
	  
  for i from 1 to coeff_list_size do
	ausgabe := sprintf(&quot;%s%s%s[%d] * blade[%d]&quot;,ausgabe,y,_opt,coeff_list[i],coeff_list[i]); 
	if i &lt; coeff_list_size then
      ausgabe := sprintf(&quot;%s + &quot;,ausgabe); 
	else
      ausgabe := sprintf(&quot;%s;&quot;,ausgabe); 
	end if;
  end do;
	  
  parse(ausgabe,statement);
  
